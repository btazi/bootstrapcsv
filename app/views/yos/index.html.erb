<p id="notice"><%= notice %></p>

<h1>Listing Yos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yos.each do |yo| %>
      <tr>
        <td><%= yo.title %></td>
        <td><%= link_to 'Show', yo %></td>
        <td><%= link_to 'Edit', edit_yo_path(yo) %></td>
        <td><%= link_to 'Destroy', yo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yo', new_yo_path %>
